{
    "name": "aspnet-helper",
    "displayName": "ASP.NET Helper",
    "description": "\"Little helper to develop faster ASP.NET MVC apps\"",
    "version": "0.5.2",
    "publisher": "schneiderpat",
    "keywords": [
        "C#", "csharp", "Razor", "Snippets"
    ],
    "galleryBanner": {
        "color": "#2c2c2c",
        "theme": "dark"
    },
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/schneiderpat/aspnet-helper"
    },
    "bugs": {
        "url": "https://github.com/schneiderpat/aspnet-helper/issues"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "cd ../aspnet-helper-server && npm run compile && cd ../aspnet-helper && tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "@types/glob":  "^5.0.30"
    },
    "dependencies": {
        "glob": "^7.1.1"
    }
}